CREATE SCHEMA
CREATE TABLE
CREATE FUNCTION
CREATE TRIGGER
CREATE SCHEMA
CREATE TABLE
CREATE TRIGGER
CREATE TRIGGER
CREATE TRIGGER

# Compare the triggers in all schemas between two databases
# Expect SQL (pseudocode):
#   Create   trigger1 on s1.table1
#   Recreate trigger2 on s1.table1
#   Drop     trigger4 on s2.table1

CREATE TRIGGER trigger1 AFTER INSERT ON s1.table1 FOR EACH ROW EXECUTE FUNCTION s1.validate1();
DROP TRIGGER trigger2 ON s1.table1;
CREATE TRIGGER trigger2 AFTER INSERT ON s1.table1 FOR EACH ROW EXECUTE FUNCTION s1.validate1();
DROP TRIGGER trigger4 ON s2.table1;


